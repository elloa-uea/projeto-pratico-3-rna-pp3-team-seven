    "c = np.arange(-4,4,0.1)\n",
    "#Implemente uma função que retorne o valor da derivada da função Leaky ReLU\n",
    "def dlrelu(c, alpha=0.01):\n",
    "    dx = np.ones_like(c)\n",
    "    dx[c < 0] = alpha\n",
    "    return dx\n",
    "\n",
    "fig, ax1 = plt.subplots(1,sharex=True,sharey=True)\n",
    "ax1.plot(dlrelu(c))\n",
    "ax1.set_title('f(x) = derivada leaky relu(x)')"
